{
  "compilerOptions":
  {
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "./dist/",
    //"noImplicitAny": true,
    "module": "es6",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "@types",
      "node_modules/@types"
    ]
  }
}