@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ex: <https://example.org/>.

<https://example.com/class1/OsobaShape> a sh:NodeShape;
    sh:targetClass <https://example.com/class1/mojeCimIri>;
    sh:closed true;
    sh:ignoredProperties (<http://www.w3.org/1999/02/22-rdf-syntax-ns#type>);
    rdfs:comment "Class 1 Popisek 1"@cs;
    rdfs:label "Class 1 Label 1"@cs.
<https://example.com/class2/AdresaShape> a sh:NodeShape;
    sh:targetClass <https://example.com/class2/adresa>;
    rdfs:comment "Adresa bydliště dané osoby"@cs;
    rdfs:label "Adresa"@cs, "Adres"@pl;
    sh:property [
  sh:maxCount 1;
  sh:path <https://example.com/Ulice>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/CisloPopisne>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#integer>
], [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Mesto>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/PSC>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#integer>
], [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Stat>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:minCount 1;
  sh:path <https://example.com/Rozloha>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#decimal>
].
<https://example.com/Jmeno-a-prijmeniShape> a sh:NodeShape;
    sh:targetClass <https://example.com/Jmeno_A_Prijmeni_class>;
    sh:closed true;
    sh:ignoredProperties (<http://www.w3.org/1999/02/22-rdf-syntax-ns#type>);
    rdfs:comment "Jméno a příjmení dané osoby"@cs;
    rdfs:label "Jméno a příjmení"@cs, "First and last name"@en;
    sh:property [
  sh:minCount 1;
  sh:maxCount 2;
  sh:path <https://example.com/Jmeno>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:minCount 1;
  sh:maxCount 2;
  sh:path <https://example.com/Prijmeni>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
].
<https://example.com/class2/AdresaShape> sh:property [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Jmeno_A_Prijmeni>;
  sh:node <https://example.com/Jmeno-a-prijmeniShape>
].
<https://example.com/class1/OsobaShape> sh:property [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Adresa>;
  sh:node <https://example.com/class2/AdresaShape>
].
<https://example.com/Narozeni-a-umrtiShape> a sh:NodeShape;
    sh:targetClass <https://example.com/Narozeni_a_umrti_class>;
    rdfs:comment "Datum narození a úmrtí dané osoby"@cs;
    rdfs:label "Datum narození a úmrtí"@cs, "Date of birth and death"@en;
    sh:property [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Narozeni>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#date>
], [
  sh:maxCount 1;
  sh:path <https://example.com/Umrti>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#date>
].
<https://example.com/class1/OsobaShape> sh:property [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Narozeni_a_umrti>;
  sh:node <https://example.com/Narozeni-a-umrtiShape>
].
<https://example.com/Vyucovaci-hodinaShape> a sh:NodeShape;
    sh:targetClass <https://example.com/Vyucovaci_hodina_class>;
    rdfs:comment "Vyučovací hodina dané osoby"@cs;
    rdfs:label "Vyučovací hodina"@cs, "Lesson"@en;
    sh:property [
  sh:maxCount 1;
  sh:path <https://example.com/Zacatek_cas>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#time>
], [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Konec_cas>;
  sh:datatype <http://www.w3.org/2001/XMLSchema#time>
].
<https://example.com/class1/OsobaShape> sh:property [
  sh:minCount 1;
  sh:maxCount 1;
  sh:path <https://example.com/Vyucovaci_hodina>;
  sh:node <https://example.com/Vyucovaci-hodinaShape>
].
