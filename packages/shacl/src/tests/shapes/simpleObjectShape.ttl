@prefix exam: <https://example.com/class1/>.
@prefix exam1: <https://example.com/class2/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

exam1:AdresaShape a sh:NodeShape;
    sh:targetClass exam1:adresa;
    rdfs:comment "Adresa bydliště dané osoby"@cs;
    rdfs:label "Adresa"@cs, "Adres"@pl;
    sh:property [
  sh:path <https://example.com/Ulice>;
  sh:maxCount 1;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:path <https://example.com/CisloPopisne>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype <http://www.w3.org/2001/XMLSchema#integer>
], [
  sh:path <https://example.com/Mesto>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
], [
  sh:path <https://example.com/PSC>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype <http://www.w3.org/2001/XMLSchema#integer>
], [
  sh:path <https://example.com/Stat>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype <http://www.w3.org/2001/XMLSchema#string>
].
exam:OsobaShape a sh:NodeShape;
    sh:targetClass exam:mojeCimIri;
    rdfs:comment "Class 1 Popisek 1"@cs;
    rdfs:label "Class 1 Label 1"@cs;
    sh:property [
  sh:path <https://example.com/Adresa>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:node exam1:AdresaShape
].
