{
  "name": "model-driven-data",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "model-driven"
  ],
  "scripts": {
    "build": "webpack -c build/webpack.config.js && cd dist && npm pack",
    "lint": "eslint --ignore-path .gitignore . --fix",
    "lint-print-config": "eslint --print-config .eslintrc.js",
    "pretest": "webpack -c build/webpack.config.js",
    "spec": "jest \"^(.*\/)*(.*\\.)*(spec)\\.[jt]s$\" --passWithNoTests",
    "integration": "jest \"^(.*\/)*(.*\\.)*(test)\\.[jt]s$\" --passWithNoTests",
    "test": "jest",
    "tsc-watch": "tsc --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@types/jsonld": "^1.5.6",
    "@types/n3": "^1.10.3",
    "js-string-escape": "^1.0.1",
    "jsonld": "^5.2.0",
    "minipass-fetch": "^1.4.1",
    "n3": "^1.11.1",
    "rdflib": "^2.2.7",
    "uri-js": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.1.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "dts-bundle-generator": "^5.9.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^27.1.1",
    "jest-raw-loader": "^1.0.1",
    "pre-commit": "^1.2.2",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browser": {
    "buffer": false
  },
  "precommit": {
    "run": "test"
  }
}
